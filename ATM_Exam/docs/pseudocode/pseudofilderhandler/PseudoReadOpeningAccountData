IMPORT removeDelimiter FROM EditFiles.py
IMPORT idColumnOpenAccountData FROM EditFiles.py

FUNCTION listAccounts
    removeDelimiter(OpeningAccountData.txt)
    index TheUserID in idColumnOpenAccountData
    RETURN accountIndex

FUNCTION listAccountsForSecondAccount
    TRY
        removeDelimiter(OpeningAccountsData.txt)
        index TheUserID in idColumnOpenAccountData + 1
        return accountIndex
    EXCEPT IndexError
        PASS