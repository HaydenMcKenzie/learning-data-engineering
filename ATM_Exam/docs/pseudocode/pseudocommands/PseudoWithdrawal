IMPORT dollarMinus FROM FloatToDollar.py
IMPORT getAccount FROM AccountUsers.py
IMPORT getSecondAccount FROM AccountUsers.py

FUNCTION minus
    IF option is "1" THEN
        Turn AccountBalance from string to floats and minus the input
        RETURN CALL dollarMinus
    ELIF option is "2"
        Turn AccountBalance from string to floats and minus the input
        RETURN CALL dollarMinus
    ENDIF